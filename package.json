{
  "name": "svelte-adapter-nekoweb",
  "version": "2.0.3",
  "description": "Addon adapter for Svelte(Kit) apps that builds the site using adapter-static and automatically deploys it to Nekoweb.",
  "keywords": [
    "adapter",
    "deploy",
    "svelte",
    "sveltekit",
    "nekoweb"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jbcarreon123/svelte-adapter-nekoweb"
  },
  "license": "MIT",
  "types": "index.d.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "axios": "^1.7.9",
    "form-data": "^4.0.2",
    "node-fetch": "^3.3.2"
  }
}
