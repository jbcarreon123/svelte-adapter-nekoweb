{
  "name": "adapter-nekoweb",
  "description": "Addon adapter for SvelteKit apps that builds the site using adapter-static and automatically deploys it to Nekoweb.",
	"keywords": [
		"adapter",
		"deploy",
		"svelte",
		"sveltekit",
    "nekoweb"
	],
  "exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./index.js"
		},
		"./package.json": "./package.json"
	},
	"types": "index.d.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^3.0.8",
    "@types/archiver": "^6.0.3",
    "@types/fs-extra": "^11.0.4",
    "archiver": "^7.0.1",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.3.0",
    "node-fetch": "^3.3.2",
    "util": "^0.12.5"
  }
}